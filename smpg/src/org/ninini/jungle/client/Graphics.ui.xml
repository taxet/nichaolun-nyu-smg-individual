<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:style field="css" type="org.ninini.jungle.client.GameCss">
        .highlighted{
          outline: dotted blue 2px;
           }
        .selected{
          outline: solid blue 3px;
           }
        td img{
          display: block
           }
    </ui:style>
    <g:HTMLPanel>
        <g:Label ui:field="whoseTurn" text="Whose turn" />
        <g:AbsolutePanel ui:field="gamePanel">
            <g:Grid ui:field="gameGrid"/>
        </g:AbsolutePanel>
        <g:Label ui:field="gameStatus" text="" />
        <g:Image ui:field="logo" />
    </g:HTMLPanel>
</ui:UiBinder>